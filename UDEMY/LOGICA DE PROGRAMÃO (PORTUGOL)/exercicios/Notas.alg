algoritmo "semnome"
// Função :
// Autor :
// Data : 15/09/2025
// Seção de Declarações 
var
aluno: caracter
nota1: real
nota2: real
media: real
inicio
// Seção de Comandos 

escreval("Digito o nome do Aluno:")
leia(aluno)

escreval("coloque a primeira nota:")
leia(nota1)

escreval("coloque a segunda nota:")
leia(nota2)

media <- (nota1+nota2)/2

escreval("a sua média é:",media)

fimalgoritmo
